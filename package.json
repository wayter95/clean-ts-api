{
  "name": "clean-node-api",
  "version": "2.3.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "up": "yarn build && docker-compose up -d",
    "stop": "docker-compose down",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand --detectOpenHandles",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "yarn test -- --watch -c jest-unit-config.js",
    "test:integration": "yarn test -- --watch -c jest-integration-config.js",
    "test:staged": "yarn test -- --findRelatedTests",
    "test:ci": "yarn test -- --coverage",
    "test:coveralls": "yarn test:ci && coveralls < coverage/lcov.info"
  },
  "keywords": [],
  "author": "Wayter Paulo <wayter.paulo.95@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/bcrypt": "^3.0.0",
    "@types/jest": "^26.0.20",
    "@types/mongodb": "^3.6.8",
    "@types/node": "^14.14.31",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.14.0",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^5.1.1",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "mockdate": "^3.0.4",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.2",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.0",
    "bcrypt": "^5.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "moesif-nodejs": "^3.1.5",
    "mongodb": "^3.6.3",
    "nodemon": "^2.0.7",
    "swagger-ui-express": "^4.1.6",
    "validator": "^13.5.2"
  },
  "engines": {
    "node": "14.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
